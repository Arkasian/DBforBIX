                       from gv$asm_disk_stat ds, gv$asm_diskgroup dg where ds.group_number=dg.group_number and ds.inst_id=dg.inst_id &#13;
                       group by dg.inst_id, dg.name&#13;
                &lt;/multiquery&gt;&#13;
	&#13;
	&lt;/server&gt;&#13;
